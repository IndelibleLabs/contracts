{
  "name": "@indeliblelabs/contracts",
  "description": "Solidity Contracts by Indelible Labs",
  "author": "Indelible Labs",
  "license": "MIT",
  "version": "0.10.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "yarn build",
    "build": "tsc && npx tsc-alias",
    "test": "yarn contracts:build && yarn contracts:test",
    "contracts:compile": "cd ./src && npx hardhat compile --force",
    "contracts:test": "cd ./src && npx hardhat test",
    "contracts:build": "ts-node ./src/scripts/build-contracts.ts && yarn contracts:compile",
    "wtf:mac": "rm -rf ./node_modules && yarn cache clean && yarn",
    "wtf:windows": "rd /s /q \"./node_modules\" && yarn cache clean && yarn"
  },
  "dependencies": {
    "@indeliblelabs/keccak256": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.5.0",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "dotenv": "^10.0.0",
    "erc721a": "4.2.3",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.2",
    "fs-extra": "^10.1.0",
    "hardhat": "^2.9.2",
    "hardhat-gas-reporter": "^1.0.8",
    "lodash": "^4.17.21",
    "merkletreejs": "^0.2.32",
    "prettier": "^2.6.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solady": "^0.0.56",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.7.20",
    "ts-node": "^10.7.0",
    "typechain": "^5.2.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.20.47",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^3.4.1"
  }
}
